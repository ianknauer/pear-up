<div class="container">
  <div class="row profile-list">
    <div class="col-12 volunteer-list">
      <%= link_to(root_path) do %>
      <p class="back-button"><%= image_tag "back.png", class: "back-icon" %> Back</p>
      <% end %>
      <div class="white-button">
        <%= image_tag "hiker_logo.png", class: "hiker-logo" %>
        <span class="search-button">Hiking</span>
      </div>
      <%= image_tag "white-tag.png", class: "add-button" %>


      <div class="white-line">
      </div>
      <%= image_tag "volun_profile@2x.png", class: "volunteer-profile", id: "open-sidr" %> <br />
      <%= image_tag "timProfile.png", class: "volunteer-profile" %> <br />

    </div>
  </div>
</div>

<div class="Rectangle-4b">
